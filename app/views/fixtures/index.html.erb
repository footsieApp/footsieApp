<p id="notice"><%= notice %></p>

<h1>Listing Fixtures</h1>

<table>
  <thead>
    <tr>
      <th>Hometeam</th>
      <th>Awayteam</th>
      <th>Date</th>
      <th>Scrapper history</th>
      <th>Competition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fixtures.each do |fixture| %>
      <tr>
        <td><%= fixture.hometeam_id %></td>
        <td><%= fixture.awayteam_id %></td>
        <td><%= fixture.date %></td>
        <td><%= fixture.scrapper_id %></td>
        <td><%= fixture.competition_id %></td>
        <td><%= link_to 'Show', fixture %></td>
        <td><%= link_to 'Edit', edit_fixture_path(fixture) %></td>
        <td><%= link_to 'Destroy', fixture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fixture', new_fixture_path %>
